                <h3>
                    <?= $question->getNameQuestion() ?>
                </h3>
                <ul>
                    <?php foreach ($question->getAnswers() as $answer) { ?>
                        <li>
                            <?= $answer->getNameAnswer() ?>
                        </li>
                    <?php } ?>
                </ul>
            <?php } ?>